<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tulun.dao.Student1Mapper">
    <select id="selectStudent1ById"  parameterType="int" resultType ="com.tulun.bean.Student1">
        select * from student where SID=#{id}
    </select>

<!--返回类型-->
    <select id="findAll"  resultType="com.tulun.bean.Student1" >
        select * from student
    </select>

</mapper>